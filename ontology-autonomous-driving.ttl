            owl:intersectionOf ( av:Person [ a owl:Restriction ;
                        owl:onProperty obo:BFO_0000196 ;
                        owl:someValuesFrom av:HumanUserRole ] ) ] ;
    skos:definition "person that has human user role"@en ;
    fibo:adaptedFrom "J3016 APR2021, section 3.31",
        "J3016 JUN2018, section 3.29"@en .

av:HumanUserRole a owl:Class ;
    rdfs:label "human user role"@en ;
    dc:contributor "Piotr Kulicki",
        "Robert Trypuz"@en ;
    dct:created "2021-08-19T14:10:00"^^xsd:dateTime ;
    dct:creator "Robert Trypuz" ;
    rdfs:comment "J3016 APR2021, section 3.31 defines this class as ``the human role in driving automation'', which is narrower than our definition."@en ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty obo:BFO_0000054 ;
            owl:someValuesFrom av:SustainedOperationOfVehicle ],
        obo:BFO_0000023 ;
    skos:definition "the (human) role that is realized in the sustained operation of a vehicle"@en ;
    fibo:adaptedFrom "J3016 APR2021, section 3.31",
        "J3016 JUN2018, section 3.29"@en ;
    fibo:explanatoryNote "The following five terms (1 - driver, 2 - passenger, 3 - DDT fallback-ready user, 4 - driverless operation dispatcher, and 5 - remote assistant) describe categories of (human) users."@en,
        "These human categories define roles that do not overlap and may be performed in varying sequences during a given trip."@en .

av:InVehicleDDTFallbackReadyUser a owl:Class ;
    rdfs:label "in-vehicle fallback-ready user"@en ;
    dc:contributor "Piotr Kulicki",
        "Robert Trypuz"@en ;
    dct:created "2021-08-19T14:10:00"^^xsd:dateTime ;
    dct:creator "Robert Trypuz" ;
